@article{Bertrand29012015,
  title = {Gender Identity and Relative Income within Households},
  author = {Bertrand, Marianne and Kamenica, Emir and Pan, Jessica},
  year = {2015},
  volume = {130},
  doi = {10.1093/qje/qjv001},
  url = {http://qje.oxfordjournals.org/content/early/2015/04/11/qje.qjv001.abstract},
  abstract = {We examine causes and consequences of relative income within households. We show that the distribution of the share of income earned by the wife exhibits a sharp drop to the right of 12, where the wife\^a\texteuro\texttrademark s income exceeds the husband\^a\texteuro\texttrademark s income. We argue that this pattern is best explained by gender identity norms, which induce an aversion to a situation where the wife earns more than her husband. We present evidence that this aversion also impacts marriage formation, the wife\^a\texteuro\texttrademark s labor force participation, the wife\^a\texteuro\texttrademark s income conditional on working, marriage satisfaction, likelihood of divorce, and the division of home production. Within marriage markets, when a randomly chosen woman becomes more likely to earn more than a randomly chosen man, marriage rates decline. In couples where the wife\^a\texteuro\texttrademark s potential income is likely to exceed the husband\^a\texteuro\texttrademark s, the wife is less likely to be in the labor force and earns less than her potential if she does work. In couples where the wife earns more than the husband, the wife spends more time on household chores; moreover, those couples are less satisfied with their marriage and are more likely to divorce. These patterns hold both cross-sectionally and within couples over time. JEL Codes: D10, J12, J16.},
  eprint = {http://qje.oxfordjournals.org/content/early/2015/04/11/qje.qjv001.full.pdf+html},
