@article{acemogluInnovationReallocationGrowth2018,
  title = {Innovation, {{Reallocation}}, and {{Growth}}},
  author = {Acemoglu, Daron and Akcigit, Ufuk and Alp, Harun and Bloom, Nicholas and Kerr, William},
