@article{tachTrendsEconomicConsequences2015,
  title = {Trends in the {{Economic Consequences}} of {{Marital}} and {{Cohabitation Dissolution}} in the {{United States}}},
  author = {Tach, Laura M. and Eads, Alicia},
  year = {2015},
