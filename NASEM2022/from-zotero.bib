@techreport{manzellaTaskbasedApproachConstructing2019,
  title = {A {{Task}}-Based {{Approach}} to {{Constructing Occupational Categories}} with {{Implications}} for {{Empirical Research}} in {{Labor Economics}}},
  author = {Manzella, Julia and Totty, Evan and Benedetto, Gary},
  year = {2019},
